{
  "name": "nord-ui",
  "version": "0.7.0",
  "title": "Nord UI",
  "description": "A Dark CSS UI framework, based on nord color palette",
  "main": "dist/nord.min.css",
  "style": "dist/nord.min.css",
  "scripts": {
    "prepublishOnly": "npm run build",
    "start": "npm run dev",
    "dev": "webpack --watch --info-verbosity verbose --config ./configs/webpack.config.dev.js",
    "build": "webpack --config ./configs/webpack.config.prod.js",
    "dev:site": "cd site && npm run dev",
    "build:site": "cd site && npm run build",
    "test": "exit 1"
  },
  "keywords": [
    "nord",
    "nordic",
    "ui",
    "css",
    "dark",
    "web",
    "framework",
    "minimal"
  ],
  "author": {
    "name": "Farhad Sharifi",
    "email": "faradidev@gmail.com",
    "web": ""
  },
  "contributors": [
    "Farhad Sharifi"
  ],
  "homepage": "https://nordui.netlify.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faraadi/nord-ui.git"
  },
  "bugs": {
    "url": "https://github.com/faraadi/nord-ui/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-fix-style-only-entries": "^0.4.0"
  },
  "dependencies": {},
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
